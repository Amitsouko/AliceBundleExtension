{
    "name": "theofidry/alice-fixtures-extension",
    "description": "Extension to load fixtures files with HautelookAliceBundle.",
    "keywords": ["Symfony", "Fixture", "ORM", "Alice", "Faker", "Behat", "Context", "Extension"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ThÃ©o FIDRY",
            "email": "theo.fidry@gmail.com",
            "homepage": "https://github.com/theofidry"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "^2.0",
        "hautelook/alice-bundle": "~1.0@beta"
    },
    "require-dev": {
        "doctrine/orm": "~2.4",
        "doctrine/doctrine-bundle": "~1.2",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "fabpot/php-cs-fixer": "~2.0@dev",
        "symfony/console": "~2.1",
        "symfony/expression-language": "^2.7",
        "symfony/framework-bundle": "~2.1",
        "symfony/yaml": "~2.1",
        "theofidry/psysh-bundle": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Fidry\\AliceFixturesExtension\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fidry\\AliceFixturesExtension\\Tests\\": "test"
        }
    }
