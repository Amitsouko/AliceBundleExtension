<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <service id="behat.alice_fixtures_extension.alice_context_initializer"
                 class="Fidry\AliceBundleExtension\Context\Initializer\AliceContextInitializer">
            <argument>%behat.alice_fixtures_extension.fixtures_base_path%</argument>
            <tag name="context.initializer"/>
        </service>

    </services>

</container>
